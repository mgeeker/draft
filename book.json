{ 
    "gitbook": ">=3.2.3",
    "description": "这只是一个手稿",
    "language": "zh-hans",
    "styles": {
        "website": "src/gitbook-slate.css"
    },
    "theme-default": {
        "showLevel": true
    },
    "plugins": [
        "gtag",
        "page-treeview",
        "expandable-chapters",
        "theme-comscore",
        "video",
        "chinese-video",
        "flexible-alerts",
        "code",
        "back-to-top-button",
        "page-footer"
    ],
    "pluginsConfig": {
        "gtag": {
            "token": "G-7713C0WZEV"
        },
        "page-treeview": {
			"copyright": "Copyright &#169; aleen42",
			"minHeaderCount": "2",
			"minHeaderDeep": "2"
		},
        "pluginsConfig": {
            "code": {
                "copyButtons": true
            }
        },
        "page-footer": {
            "description": "Last Modified",
            "signature": "MacGeeker",
            "wisdom": "文章可自由转载，但请在文章中带上原文链接！",
            "format": "yyyy-MM-dd hh:mm:ss",
            "copyright": "Copyright &#169; macgeeker",
            "timeColor": "#666",
            "copyrightColor": "#666",
            "utcOffset": "8",
            "isShowQRCode": false,
            "baseUri": "https://macgeeker.com/",
            "isShowIssues": false,
            "repo": "mgeeker/draft",
            "issueNum": "8",
            "token": "",
            "style": "normal"
        }
    }
}